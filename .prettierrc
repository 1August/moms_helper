{
  "printWidth": 80,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "bracketSpacing": true,
  "semi": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "useTabs": true,
  "bracketSameLine": false,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-organize-imports"
  ]
}
